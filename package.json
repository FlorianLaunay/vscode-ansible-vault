{
	"name": "ansible-vault",
	"version": "1.0.1",
	"publisher": "flaunay",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.39.0"
	},
	"license": "SEE LICENSE IN LICENSE",
	"displayName": "Ansible Vault",
	"description": "Encrypt/decrypt files with ansible vault, supports vault-id",
	"categories": [
		"Other"
	],
	"keywords": [
		"ansible",
		"ansible-vault",
		"decrypt",
		"encrypt",
		"multi-root ready"
	],
	"preview": false,
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "ansible-vault-inline",
			"properties": {
				"ansibleVaultInline.executable": {
					"type": "string",
					"default": "ansible-vault",
					"description": "Full path of ansible-vault executable"
				}
			}
		},
		"commands": [
			{
				"command": "extension.ansibleVaultInline",
				"title": "Encrypt/Decrypt",
				"category": "Ansible Vault"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.ansibleVaultInline",
					"when": "editorTextFocus"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.ansibleVaultInline",
				"key": "ctrl+alt+0",
				"mac": "cmd+alt+0",
				"when": "editorTextFocus"
			}
		]
	},
	"activationEvents": [
		"onCommand:extension.ansibleVaultInline"
	],
	"markdown": "github",
	"qna": "marketplace",
	"dependencies": {
		"@types/ini": "^1.3.30",
		"@types/tmp": "^0.1.0",
		"ini": "^1.3.5",
		"tmp": "^0.1.0",
		"untildify": "^4.0.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.0",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^6.1.3",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/FlorianLaunay/vscode-ansible-vault.git"
	}
}
